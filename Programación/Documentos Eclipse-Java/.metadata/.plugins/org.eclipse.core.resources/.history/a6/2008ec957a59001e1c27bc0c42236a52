
public class Ejemplovariable {

	public static void main(String[] args) {
		// TODO Vamos a declarar variables
		byte barco = 12, avion = 27;
		short queso = 8000, jamon = 9000;
		int hola = 100000, adios = 120000;
		long pedro = 1000000, ramon = 1200000;
		double casa = 3.5, techo = 7.9;
		float fabrica = 8.2578f, puente = 195.3865f;
		char elfo = 'e', orco = 'o';
		boolean cerca = true, lejos = false;
		
		System.out.println(barco, avion, queso, jamon, hola, adios, pedro, ramon, casa, techo, fabrica, puente, elfo, orco, cerca, lejos);
		
		

	}

}
