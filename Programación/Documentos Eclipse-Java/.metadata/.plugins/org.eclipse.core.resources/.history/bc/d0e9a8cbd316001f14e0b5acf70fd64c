package ejercicio1;

import java.time.LocalDate;
import java.util.ArrayList;

/*Autor: Joshua Sangareau Quesada
Fecha: 20 may 2024 
Descripcion: clase SocioFutbol
*/
public class SocioFutbol extends Socio implements Comparable{

	
	private String nombreEquipo;
	private ArrayList<PartidoFutbol> partidosFutbol;
	
	
	public SocioFutbol(String nombre, String dni, String telefono, String nombreEquipo, ArrayList<PartidoFutbol> partidosFutbol) {
		super(nombre, dni, telefono);
		this.nombreEquipo = nombreEquipo;
		this.partidosFutbol = partidosFutbol;
	}


	@Override
	public int compareTo(Object o) {
		return this.nombre.compareTo(o.);
	}
	
	
	
	
}
