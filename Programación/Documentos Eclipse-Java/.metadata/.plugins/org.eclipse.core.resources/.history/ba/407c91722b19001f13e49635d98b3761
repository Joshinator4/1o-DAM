package ejercicio1;

/*Autor: Joshua Sangareau Quesada
Fecha: 23 may 2024 
Descripcion: clase Coche
*/
public class Coche extends Vehiculo{
	
	
	enum Estado {NUEVO, DAÑADO_PARCIAL, DAÑADO};

	private int kilometros;
	private Estado estado;
	
	public Coche(String matricula, String dni, String telefono,String modelo, int kilometros, Estado estado) {
		super(matricula, dni, telefono, modelo);
		this.kilometros = kilometros;
		this.estado = estado;
	}
	
	
}
